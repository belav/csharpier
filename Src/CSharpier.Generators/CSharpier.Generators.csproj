<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
        <PackageScribanIncludeSource>true</PackageScribanIncludeSource>
    </PropertyGroup>
    
    <ItemGroup>
        <PackageReference Include="Microsoft.CodeAnalysis.Common" Version="3.8.0" />
        <PackageReference Include="Scriban" Version="3.9.0" IncludeAssets="Build" />
    </ItemGroup>

    <ItemGroup>
        <EmbeddedResource Include="@(None -&gt; WithMetadataValue('Extension', '.sbntxt'))" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\Generators\Generators.csproj" OutputItemType="Analyzer" />
    </ItemGroup>
</Project>
